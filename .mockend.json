{
	"User": {
		"id": {
			"int": {
				"min": 1,
				"max": 99999999999999
			}
		},
		"firstName": { "regexp": "[A-Z][a-z]{5,10}" },
		"lastName": { "regexp": "[A-Z][a-z]{5,10}" },
		"email": {
			"regexp": "#[a-z]{5,10}@[a-z]{5}\\.[a-z]{2,3}"
		},
		"phone": {
			"int": {
				"min": 1000000000,
				"max": 9999999999
			}
		},
		"company": {
			{ "regexp": "[A-Z][a-z]{4,14}" }
		}
	}
}
